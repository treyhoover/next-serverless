{
  "scripts": {
    "dev": "cross-env ENV_FILE=.env.local nodemon server/devServer.ts",
    "css:build": "node ./css/buildTachyons",
    "build": "yarn css:build && cross-env ENV_FILE=.env.production next build && tsc --project tsconfig.server.json",
    "lambda": "serverless offline start",
    "static:deploy": "aws s3 sync ./static s3://next-serverless-dev/static --profile serverless",
    "deploy": "yarn static:deploy && serverless deploy --aws-profile serverless"
  },
  "dependencies": {
    "@rehooks/component-size": "^1.0.2",
