    "@zeit/next-css": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "compression": "^1.7.3",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "lodash.debounce": "^4.0.8",
    "next": "^8.0.1",
    "next-compose-plugins": "^2.1.1",
    "nodemon": "^1.18.10",
    "path-match": "^1.2.4",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-http": "^1.9.0",
    "serverless-offline": "^4.4.0",
    "serverless-plugin-include-dependencies": "^3.2.1",
    "tachyons-generator": "^0.23.0",
